generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ContactMessage {
  id             Int      @id @default(autoincrement())
  gender         String
  lastName       String
  firstName      String
  email          String
  phone          String
  messageType    String
  message        String
  availabilities String
  createdAt      DateTime @default(now())
}
